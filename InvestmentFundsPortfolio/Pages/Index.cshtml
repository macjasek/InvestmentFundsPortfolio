@page
@model IndexModel
@using HtmlAgilityPack;
@{
    ViewData["Title"] = "Home page";

}

<div>
    @foreach (var f in Model.PortfolioFunds)
    {
        <h2>@f.Name</h2>
        <p>Price: @f.Price</p>
        <p>Starting Price: @f.StartingPrice</p>
        <h3>Profit/Loss: @(f.Price - f.StartingPrice)</h3>
    }

</div>
